{
    "name": "asuna-accessories",
    "private": true,
    "description": "",
    "license": "",
    "sideEffects": false,
    "scripts": {
        "start": "remix-serve build",
        "dev": "run-p dev:*",
        "dev:remix": "cross-env NODE_ENV=development remix dev",
        "dev:css": "cross-env NODE_ENV=development npm run generate:css -- --watch",
        "build": "run-s build:*",
        "build:css": "npm run generate:css -- --minify",
        "build:remix": "remix build",
        "deploy": "fly deploy --remote-only",
        "cdk:start": "sst start --stage asuna-demo-dev",
        "cdk:build": "sst build",
        "cdk:deploy": "sst deploy --stage asuna-demo-staging",
        "cdk:remove": "sst remove",
        "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
        "format": "npx prettier --write ."
    },
    "dependencies": {
        "@aws-sdk/client-eventbridge": "3.88.0",
        "@aws-sdk/client-kms": "3.87.0",
        "@aws-sdk/client-sqs": "3.87.0",
        "@prisma/client": "3.14.0",
        "@remix-run/node": "1.4.3",
        "@remix-run/react": "1.4.3",
        "@remix-run/serve": "1.4.3",
        "@types/aws-lambda": "8.10.97",
        "asn1.js": "5.4.1",
        "compression": "1.7.4",
        "ethers": "5.6.4",
        "ethers-aws-kms-signer": "1.3.2",
        "express": "4.18.1",
        "graphql-request": "4.2.0",
        "js-cookie": "3.0.1",
        "morgan": "1.10.0",
        "mysql2": "2.3.3",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "uuid": "8.3.2"
    },
    "devDependencies": {
        "@remix-run/dev": "1.4.3",
        "@remix-run/eslint-config": "1.4.3",
        "@serverless-stack/cli": "1.2.15",
        "@serverless-stack/resources": "1.2.15",
        "@types/compression": "1.7.2",
        "@types/express": "4.17.13",
        "@types/js-cookie": "3.0.2",
        "@types/morgan": "1.9.3",
        "@types/node": "17.0.29",
        "@types/react": "17.0.24",
        "@types/react-dom": "17.0.9",
        "@types/uuid": "8.3.4",
        "aws-cdk-lib": "2.24.0",
        "cross-env": "7.0.3",
        "eslint": "8.11.0",
        "npm-run-all": "4.1.5",
        "prettier": "2.6.2",
        "prisma": "3.14.0",
        "rome": "0.5.0-next",
        "tailwindcss": "3.0.24",
        "ts-node": "10.7.0",
        "typescript": "4.5.5"
    },
    "engines": {
        "node": ">=14"
    },
    "prisma": {
        "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
    },
    "prettier": {
        "arrowParens": "avoid",
        "bracketSpacing": true,
        "printWidth": 120,
        "semi": false,
        "singleQuote": false,
        "trailingComma": "es5",
        "tabWidth": 4
    }
}
